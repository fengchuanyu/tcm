
.big{
  margin:0;
  padding:0;
  background: #f2f2f2;
}
/*title开始*/
.title{
  background: #ffffff;
  text-align: center;
  height:100rpx;
  font-size:36rpx;
  font-weight: 500;
  line-height: 100rpx;
  margin-bottom:25rpx;
}
.body{
  height: 300rpx;
  background: #ffffff;
}
.nowtime{
  /* background: #ffffff; */
  padding:25rpx 0;
  height:50rpx;
  border-bottom:1px solid #bfbfbf;
  position: relative;
}
.picker{
  position:relative;
  font-size:32rpx;
  color:#898989;
  padding:0 30rpx;
  line-height: 50rpx;
}
.select{
  height: 40rpx;
  width: 40rpx;
  position:absolute;
  left:210rpx;
  top:50%;
  transform: translate(0,-50%);
}
.classfiy{
  position:absolute;
  height:56rpx;
  border:1px solid #fed5d5;
  font-size:30rpx;
  right:30rpx;
  top:22rpx;
}
.classfiy text{
  line-height: 56rpx;
  padding:10rpx;
  background: #ffffff;
  color:#fed5d5;
}
.classfiy .now{
  background: #fed5d5;
  color:#ffffff;
}

.doctor{
  margin-top:20rpx;
  /* border-bottom:1rpx solid #bfbfbc; */
}
.doctor-inf{
  margin:10rpx 30rpx;
  overflow: hidden;
  position: relative;
  border-bottom:1rpx solid #b4b4b4;
}
.doctor-inf image{
  height: 120rpx;
  width: 120rpx;
  border-radius: 10rpx;
}
.doctor-infname{
  float:left;
  height: 120rpx;
  /* line-height: 150rpx; */
  position:absolute;
  top:50%;
  left:180rpx;
  transform: translate(0%,-50%);
}
.infname{
  height: 120rpx;
  line-height: 120rpx;
}


/*titile结束*/