
.big{
  margin:0;
  padding:0;
  background: #f2f2f2;
  padding-top:20rpx;
}


/*body开始*/
.doctor-big{
  background: #ffffff;
}
.doctor-inf{
  margin:0 30rpx;
  overflow: hidden;
  padding:30rpx 0;
  position: relative;
}
.doctor-inf image{
  height: 120rpx;
  width: 120rpx;
  border-radius: 10rpx;
}
.doctor-infname{
  float:left;
  height: 100rpx;
  position:absolute;
  top:50%;
  left:150rpx;
  transform: translate(0%,-50%);
}
.infname{
  height: 60rpx;
  line-height: 60rpx;
}
.infpost{
  height: 60rpx;
  line-height: 60rpx;
  font-size:30rpx;
}

.classfiy{
  padding:20rpx 30rpx 2rpx 30rpx;
  overflow: hidden;
}
.classfiy view{
  padding-bottom:30rpx;
  text-align: center;
  
}
.classfiy view:first-child{
  margin-left:150rpx;
  float:left;
}
.classfiy view:last-child{
  float:right;
  margin-right:150rpx;
}
.classfiy view text{
  padding:0 20rpx 30rpx 20rpx;
  font-size:35rpx;
}
.spe{
  border-bottom:1rpx solid #fed5d5;
  color:#fed5d5;
}


.orderinfs{
  margin-top:30rpx;
  background: #ffffff;
}
.orderinf{
  overflow: hidden;
  line-height: 100rpx;
  height: 100rpx;
  margin:10rpx 30rpx;
  position: relative;
  border-bottom:1rpx solid #f8f8f8;
}
.orderinf view{
  overflow: hidden;
  float:left;
  font-size:35rpx;
}
.docotr-but{
  font-size:30rpx;
  position:absolute;
  right:0rpx;
  top:50%;
  transform: translate(0%,-50%);
}
.docotr-but text{
  margin-left:20rpx;
  color:#ffffff;
  line-height: 56rpx;
  padding:10rpx;
  width:100rpx;
  border-radius: 5px;
}
.doctor-num{
  border:1rpx solid #fed5d5;
  background: #fed5d5;
  font-size:30rpx;
}
.doctor-money{
  border:1rpx solid #ff9600;
  background: #ff9600;
  font-size:30rpx;
}
/*body结束*/