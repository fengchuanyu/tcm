/* client/pages/dataorder/dataorder.wxss */
.big{
  margin:0;
  padding:0;
  background: #f2f2f2;
  padding-top:20rpx;
}
/*title开始*/
.body{
  /* height: 300rpx; */
  background: #ffffff;
}
.nowtime{
  padding:25rpx 0;
  height:50rpx;
  border-bottom:1px solid #f8f8f8;
  position: relative;
}
.picker{
  position:relative;
  font-size:32rpx;
  color:#898989;
  padding:0 30rpx;
  line-height: 50rpx;
  height:50rpx;
}
.select{
  height: 40rpx;
  width: 40rpx;
  position:absolute;
  left:210rpx;
  top:50%;
  transform: translate(0,-50%);
}
.classfiy{
  position:absolute;
  border:1px solid #fed5d5;
  font-size:30rpx;
  right:30rpx;
  width:325rpx;
  top:22rpx;
  margin:0;
  overflow: hidden;
  height: 56rpx;
}
.classfiy text{
  line-height: 56rpx;
  display: block;
  height: 56rpx;
  padding:0 9rpx;
  background: #ffffff;
  color:#fed5d5;
  float:left;
}
.classfiy .now{
  background: #fed5d5;
  color:#ffffff;
}
.time{
  border-bottom:1px solid #f8f8f8;
}
.weeks{
  height: 50rpx;
  text-align: center;
  line-height: 50rpx;
  padding:25rpx 30rpx;
  border-bottom:1px solid #f8f8f8;
}
.date{
  text-align: center;
  padding:20rpx;
  margin-left:10rpx;
  overflow: hidden;
}
.week{
  font-size:30rpx;
  float:left;
  width:14%;
}
.week-date view{
  height: 30rpx;
  line-height: 30rpx;
}
.week-date view:first-child{
  margin-bottom:20rpx;
}
.week-date{
  padding:15rpx 18.5rpx;
  font-size:30rpx;
  float:left;
  border-radius: 50%;
  width:9%;
}
.active{
  background: #fed5d5;
  color:#ffffff;
}

.doctor{
  margin-top:20rpx;
  border-bottom:1px solid #f8f8f8;
  overflow: hidden;
}
.doctor-inf{
  margin:10rpx 30rpx;
  overflow: hidden;
  position: relative;
  border-bottom:1rpx solid #f8f8f8;
}
.doctor-inf image{
  height: 120rpx;
  width: 120rpx;
  border-radius: 10rpx;
}
.doctor-infname{
  float:left;
  height: 120rpx;
  position:absolute;
  top:50%;
  left:160rpx;
  transform: translate(0%,-50%);
}
.infname{
  height: 60rpx;
  line-height: 60rpx;
  font-size:35rpx;
}
.infpost{
  height: 75rpx;
  line-height: 75rpx;
  font-size:30rpx;
}
.docotr-but{
  font-size:30rpx;
  position:absolute;
  right:0rpx;
  top:50%;
  transform: translate(0%,-50%);
}
.docotr-but text{
  margin-left:20rpx;
  color:#ffffff;
  line-height: 56rpx;
  padding:10rpx;
  width:100rpx;
  border-radius: 5px;
}
.doctor-num{
  border:1rpx solid #fed5d5;
  background: #fed5d5;
}
.doctor-money{
  border:1rpx solid #ff9600;
  background: #ff9600;
}
/*titile结束*/